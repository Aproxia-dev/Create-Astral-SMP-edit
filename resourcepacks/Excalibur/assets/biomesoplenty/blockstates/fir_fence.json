{
    "multipart": [
        {   "apply": { "model": "biomesoplenty:block/fir_fence_post" }},
        {   "when": { "north": "true" },
            "apply": { "model": "biomesoplenty:block/fir_fence_side" }
        },
        {   "when": { "east": "true" },
            "apply": { "model": "biomesoplenty:block/fir_fence_side", "y": 90 }
        },
        {   "when": { "south": "true" },
            "apply": { "model": "biomesoplenty:block/fir_fence_side", "y": 180 }
        },
        {   "when": { "west": "true" },
            "apply": { "model": "biomesoplenty:block/fir_fence_side", "y": 270 }
        }
    ]
}
